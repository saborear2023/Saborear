<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp">

    <ImageView
        android:id="@+id/subcadastroutilizacao_campo"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:importantForAccessibility="no"
        android:scaleType="fitXY"
        android:tint="#81F8F8F8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/saborear_campo_08"
        tools:ignore="UseAppTint,ImageContrastCheck" />

    <TextView
        android:id="@+id/subcadastroutilizacao_texto"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:importantForAccessibility="no"
        android:scrollbars="vertical"
        android:textColor="#BF000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/subcadastroutilizacao_campo"
        app:layout_constraintEnd_toStartOf="@+id/subcadastroutilizacao_remover"
        app:layout_constraintStart_toStartOf="@+id/subcadastroutilizacao_campo"
        app:layout_constraintTop_toTopOf="@+id/subcadastroutilizacao_campo" />

    <ImageButton
        android:id="@+id/subcadastroutilizacao_remover"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="#00FFFFFF"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/subcadastroutilizacao_campo"
        app:layout_constraintEnd_toEndOf="@+id/subcadastroutilizacao_campo"
        app:layout_constraintTop_toTopOf="@+id/subcadastroutilizacao_campo"
        app:srcCompat="@drawable/saborear_icon_remover"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>
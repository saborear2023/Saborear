<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="160dp">

    <ImageView
        android:id="@+id/subreceitagrande_campo_01"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:importantForAccessibility="no"
        android:scaleType="fitXY"
        android:tint="#FF0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/saborear_retangulo_03"
        tools:ignore="UseAppTint,ImageContrastCheck" />

    <ImageView
        android:id="@+id/subreceitagrande_campo_02"
        android:layout_width="0dp"
        android:layout_height="64dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:importantForAccessibility="no"
        android:scaleType="fitXY"
        android:tint="#FFE6E6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/subreceitagrande_campo_01"
        app:srcCompat="@drawable/saborear_retangulo_04"
        tools:ignore="UseAppTint,ImageContrastCheck" />

    <ImageView
        android:id="@+id/subreceitagrande_estrela_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/subreceitagrande_nota"
        app:layout_constraintStart_toEndOf="@+id/subreceitagrande_nota"
        app:layout_constraintTop_toBottomOf="@+id/subreceitagrande_nome"
        app:srcCompat="@drawable/saborear_icon_estrela"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/subreceitagrande_estrela_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/subreceitagrande_estrela_01"
        app:layout_constraintStart_toEndOf="@+id/subreceitagrande_estrela_01"
        app:layout_constraintTop_toTopOf="@+id/subreceitagrande_estrela_01"
        app:srcCompat="@drawable/saborear_icon_estrela"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/subreceitagrande_estrela_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="@+id/subreceitagrande_estrela_02"
        app:layout_constraintStart_toEndOf="@+id/subreceitagrande_estrela_02"
        app:layout_constraintTop_toTopOf="@+id/subreceitagrande_estrela_02"
        app:srcCompat="@drawable/saborear_icon_estrela"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/subreceitagrande_imagem"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="24dp"
        android:importantForAccessibility="no"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/subreceitagrande_campo_02"
        app:layout_constraintStart_toStartOf="@+id/subreceitagrande_campo_01"
        app:layout_constraintTop_toTopOf="@+id/subreceitagrande_campo_01" />

    <TextView
        android:id="@+id/subreceitagrande_nome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/alata"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/subreceitagrande_campo_01"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/subreceitagrande_imagem"
        app:layout_constraintTop_toTopOf="@+id/subreceitagrande_imagem" />

    <TextView
        android:id="@+id/subreceitagrande_nota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/alata"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/subreceitagrande_imagem"
        app:layout_constraintTop_toBottomOf="@+id/subreceitagrande_nome" />

    <ProgressBar
        android:id="@+id/subreceitagrande_carregar"
        style="?android:attr/progressBarStyle"
        android:layout_width="32dp"
        android:layout_height="0dp"
        android:indeterminateTint="#000000"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/subreceitagrande_imagem"
        app:layout_constraintEnd_toEndOf="@+id/subreceitagrande_imagem"
        app:layout_constraintStart_toStartOf="@+id/subreceitagrande_imagem"
        app:layout_constraintTop_toTopOf="@+id/subreceitagrande_imagem" />
</androidx.constraintlayout.widget.ConstraintLayout>